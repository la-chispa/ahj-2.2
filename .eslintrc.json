{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "settings": {
    "import/extensions": [
      ".js"
    ]
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}